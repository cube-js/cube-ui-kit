{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "CommonJS",
    "outDir": "./dist/cjs",
    "noEmit": false,
    "declaration": false
  },
  "exclude": [
    "src/**/__mocks__/**/*",
    "src/**/*.stories.tsx",
    "src/**/*.stories.ts",
    "src/test/**/*",
    "src/**/*.test.tsx",
    "src/**/*.test.ts",
    "src/antd.js",
    "src/storybook.tsx",
    "src/App.tsx",
    "src/script.js"
  ]
}
