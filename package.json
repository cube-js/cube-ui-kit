{
  "name": "@cube-dev/ui-kit",
  "version": "0.8.0",
  "description": "UIKit for Cube Projects",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/mjs/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/mjs/index.js",
      "types": "./dist/mjs/index.d.ts"
    }
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.map",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "start": "cross-env PARCEL_AUTOINSTALL=false parcel src/storybook.html",
    "build": "cross-env NODE_ENV=production rollup -c",
    "build:esm": "cross-env NODE_ENV=production ESM_BUILD_ONLY=true rollup -c",
    "watch": "rollup -c -w",
    "test": "jest",
    "test-cover": "jest --coverage",
    "test-watch": "jest --watch",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint --cache src && prettier --check \"src/**/*.{js,jsx}\"",
    "fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\" && eslint --fix --cache src --ext .js,.ts,.jsx,.tsx",
    "compile": "babel src -d dist --copy-files",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "npx chromatic",
    "size": "size-limit",
    "prepare": "husky install"
  },
  "keywords": [
    "uikit"
  ],
  "author": "Cube Dev Team",
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@commitlint/cli": "16.1.0",
