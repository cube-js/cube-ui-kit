    "react-aria": "^3.30.0",
    "react-focus-lock": "^2.9.4",
    "react-is": "^17.0.0",
    "react-keyed-flatten-children": "^1.3.0",
    "react-stately": "^3.28.0",
    "react-transition-group": "^4.4.5",
    "react-types": "^0.1.0",
    "storybook": "^7.0.17",
    "tiny-invariant": "^1.3.1",
    "valid-url": "^1.0.9"
