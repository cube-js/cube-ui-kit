    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^6.22.3",
    "react-test-renderer": "^19.1.1",
    "rimraf": "^6.0.1",
    "size-limit": "^8.2.6",
    "storybook": "^9.1.2",
    "storybook-addon-turbo-build": "^2.0.1",
    "styled-components": "^6.1.19",
    "swc-loader": "^0.2.6",
    "typescript": "^5.6.3",
    "vite": "^7.1.3"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari major versions"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=22.14.0",
    "pnpm": "^10.0.0"
  }
}