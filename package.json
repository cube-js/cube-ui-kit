    "@swc/jest": "^0.2.29",
    "@testing-library/dom": "^9.0.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^8.0.0",
