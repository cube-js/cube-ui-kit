    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^6.2.1",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "size-limit": "^8.1.0",
    "storybook-addon-turbo-build": "^2.0.1",
    "styled-components": "^6.1.8",
    "swc-loader": "^0.2.3",
    "typescript": "^4.5.4",
    "vite": "^4.4.9",
    "webpack": "^5.76.1"
  },
  "resolutions": {
